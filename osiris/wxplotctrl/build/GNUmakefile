ifeq ($(WX_SHARED),0)
../lib/libwxplotctrl.a: $(WXPLOTCTRL_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXPLOTCTRL_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_wxplotctrl_lib: 
	$(INSTALL) -d $(prefix)/lib
	$(INSTALL) -m 644 ../lib/libwxplotctrl.a $(prefix)/lib
endif

ifeq ($(WX_SHARED),1)
../lib/libwxplotctrl.so: $(WXPLOTCTRL_DLL_OBJECTS)
	$(CXX) -shared -fPIC -o $@ $(WXPLOTCTRL_DLL_OBJECTS) $(LDFLAGS) $(__DEBUGINFO) -pthread -L../lib  $(WX_LIBS)
endif

ifeq ($(WX_SHARED),1)
install_wxplotctrl_dll: 
	$(INSTALL) -d $(prefix)/lib
	$(INSTALL) -m 644 ../lib/libwxplotctrl.so $(prefix)/lib
	install -c ../lib/libwxplotctrl.so $(prefix)/lib
endif

